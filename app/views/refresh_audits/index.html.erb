<h1>Listing refresh_audits</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Stamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @refresh_audits.each do |refresh_audit| %>
      <tr>
        <td><%= refresh_audit.period %></td>
        <td><%= refresh_audit.stamp %></td>
        <td><%= link_to 'Show', refresh_audit %></td>
        <td><%= link_to 'Edit', edit_refresh_audit_path(refresh_audit) %></td>
        <td><%= link_to 'Destroy', refresh_audit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Refresh audit', new_refresh_audit_path %>
